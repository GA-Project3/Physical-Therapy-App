<div class="container">

<h2>Add New Exercise</h2>

	<%= form_for :exercises do |f| %>
	<%= f.text_field :title, placeholder: "Exercise" %><br>
	<%= f.text_area :description, placeholder: "Description" %> <br>
	<%= f.text_field :image_url, placeholder: "Exercise Image" %>
	<%= f.submit "Submit" %>
<% end %>

</div>