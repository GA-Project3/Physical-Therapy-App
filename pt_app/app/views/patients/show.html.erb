<img class = 'image' src="<%= @patient.image_url %>" alt="image for patient" >
<li><%= @patient.first_name + " " + @patient.last_name %></li>
<li>Email: <%= @patient.email %></li>
<li>Phone Number: <%= @patient.phone %></li>
<li>Address: <%= @patient.location %></li>
<li>Diagnosis <%= @patient_diagnosis %></li>

<div>
<% if current_user != nil %>
	<% if current_user.user_t == 'patients' %>
		<div class = "button">
			<%= link_to "Edit Profile", edit_patient_path %>
		</div>
		<%else%>
	<%end%>
	<%end%>
</div>

<div>
	List of Exercises:
</div>

<div class="row">

  <% @exercises.each do |exercise| %>
  <div class='col-md-1'>
    <img src="<%= exercise.image_url %>" width="100px"/>
  </div>
  <div class='col-md-6'>
    <h6><%= exercise.title %></h6>
    <%= exercise.description %>
  <% end %>

</div>

<div>
	Your Current Physician:
	<!-- Doctor.find(Patient.doctor_id) -->
</div>

